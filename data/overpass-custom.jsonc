{
    "osm": {
        "HigayodoStreetMap2025": {
            "overpass": [
                "nwr[\"amenity\"]",
                "nwr[\"shop\"]",
                "nwr[\"craft\"]",
                "nwr[\"tourism\"]",
                "nwr[\"office\"]",
                "node[\"railway\"=\"station\"]",
                "nwr[\"leisure\"]"
            ],
            "tags": [
                "shop",
                "amenity!=parking",
                "amenity!=vending_machine",
                "amenity!=parking_entrance",
                "amenity!=bench",
                "craft",
                "office",
                "amenity",
                "tourism",
                "railway",
                "leisure"
            ],
            "expression": {
                "poiView": true, // true時はアイコン表示
                "viewArea": false,
                "stroke": "#A06060",
                "stroke-width": 1,
                "fill-opacity": 0.25
            },
            "titles": [
                "name",
                "operator",
                "note"
            ]
        },
        "Green": {
            "overpass": [
                "nwr[\"natural\"=\"tree_row\"]",
                "nwr[\"barrier\"=\"hedge\"]"
            ],
            "tags": [
                "natural",
                "barrier"
            ],
            "expression": {
                "poiView": false, // true時はアイコン表示
                "viewArea": false,
                "stroke": "#50A050",
                "stroke-width": 3,
                "fill-opacity": 0.25
            },
            "titles": [
                "name",
                "operator"
            ]
        }
    }
}